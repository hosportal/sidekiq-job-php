        "knplabs/phpspec-welldone-extension": "dev-master",
        "phpspec/nyan-formatters": "^1.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "^1.01",
        "m6web/redis-mock": "^2.2"
    },
    "autoload": {
        "psr-4": {"SidekiqJob\\": "src/"}
