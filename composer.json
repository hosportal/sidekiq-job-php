{
    "name": "hosportal/sidekiq-job-php",
    "description": "Push and schedule jobs to Sidekiq from PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damjan Znidarsic",
            "email": "damjan@znidarsic.me"
        }
    ],
    "keywords" : [
        "sidekiq",
        "resque",
        "redis",
